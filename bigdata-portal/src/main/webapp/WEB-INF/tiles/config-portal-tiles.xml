<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
       
<tiles-definitions>
	
	<!-- 유제품 소비 및 트렌드 분석 -->
	<definition name="dct/*" extends="single.tiles">
		<put-attribute name="title" value="유제품 소비 및 트렌드 분석" />
		<put-attribute name="header" value="/WEB-INF/jsp/portal/dct/include/top.jsp" />
		<put-attribute name="left" value="/WEB-INF/jsp/layout/empty.jsp" />
		<put-attribute name="body" value="/WEB-INF/jsp/portal/dct/{1}.jsp" />
		<put-attribute name="footer" value="/WEB-INF/jsp/portal/dct/include/bottom.jsp" />
	</definition>

	<!-- 귀농귀촌 지원 서비스 -->
	<definition name="frtn/*/*" extends="single.tiles">
		<put-attribute name="title" value="귀농귀촌 지원 서비스" />
		<put-attribute name="header" value="/WEB-INF/jsp/portal/frtn/include/top.jsp" />
		<put-attribute name="left" value="/WEB-INF/jsp/layout/empty.jsp" />
		<put-attribute name="body" value="/WEB-INF/jsp/portal/frtn/{1}/{2}.jsp" />
		<put-attribute name="footer" value="/WEB-INF/jsp/portal/frtn/include/bottom.jsp" />
	</definition>
	<definition name="single/frtn/*/*" extends="single.tiles">
		<put-attribute name="body" value="/WEB-INF/jsp/portal/frtn/{1}/{2}.jsp" />
	</definition>
	<definition name="single/frtn/*/*/*" extends="single.tiles">
		<put-attribute name="body" value="/WEB-INF/jsp/portal/frtn/{1}/{2}/{3}.jsp" />
	</definition>	
	
</tiles-definitions>